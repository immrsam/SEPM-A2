package a2;
import java.util.Scanner;

public class main {

	public static main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Assignment 2");
		System.out.print("Input: ");
		String value = input.nextLine();
		System.out.println("-- " + value + " --");
		input.close();
	}
}
